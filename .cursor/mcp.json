{
  "mcpServers": {
    "Bright Data": {
      "command": "npx",
      "args": ["@brightdata/mcp"],
      "env": {
        "API_TOKEN": "<your-api-token-here>"
      }
    },
    "anki-mcp": {
      "command": "npx",
      "args": ["-y", "anki-mcp-http", "--stdio"],
      "env": {
        "ANKI_CONNECT_URL": "http://localhost:8765"
      }
    },
    "Github": {
        "url": "https://api.githubcopilot.com/mcp/"
    },
    "remote-proxy": {
        "command": "npx",
        "args": [
            "mcp-remote",
            "http://example.com/mcp",
            "--header",
            "Authorization: Bearer 123"
      ]
    }
  }
}